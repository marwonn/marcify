# Spotify API Credentials
# Get these from https://developer.spotify.com/dashboard
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret

# Redirect URL for Spotify OAuth
# Must match the redirect URI in your Spotify app settings
REDIRECT_URL=http://localhost:5000/spotify-oauth2callback

# Flask secret key (use a random string)
SECRET_KEY=your_random_secret_key

# Last.fm API Key
# Get this from https://www.last.fm/api/account/create
LASTFM_API_KEY=your_lastfm_api_key

# Your Spotify username
USERNAME=your_spotify_username
